def function(x, y):
	return x + y + DEVICE_NUMBER*100

def main():
	import numpy
	gpu_list = get_GPU_list(2)
	result = function(x, y).range(x=0:10,y=0:10)
							.split(result,x=2,y=2)
							.execid(gpu_list)
	flag = False
	for i in range(10):
		for j in range(10):
			while True:
				if result[i][j] > 100:
					result[i][j] -= 100
					flag = True
				else:
					break

	correct = numpy.empty((10,10), dtype=numpy.float32)
	
	for i in range(10):
		for j in range(10):
			correct[i][j] = i+j

	if (correct == result).all():
		print "Ok"
		if flag == False:
			print "Only one GPU is used, maybe problem"
	else:
		print "Fail"
		print "Correct"
		print correct
		print "Result"
		print result


def func(a, x, y):
	max = 0
	for z in range(10):
		v = point_query_3d(a, x, y, z)
		if max < v:
			max = v
	return max


def ft(front, back, x, y):
	f = point_query_2d(front, x, y)
	b = point_query_2d(back, x, y)
	return f + b

def main():
	import numpy
	n = 10
	a = numpy.ones((n,n,n),dtype=numpy.float32)
	for i in range(n):
		for j in range(n):
			for k in range(n):
				a[i][j][k] = i + j + k

	result = func(a, x, y).split(a, z=4)
					.range(x=0:n,y=0:n)
					.merge(ft)

	correct = numpy.ones((n,n), dtype=numpy.float32)
	for i in range(n):
		for j in range(n):
			correct[i][j] = i*4 + j*4 + 20

	if (correct == result).all():
		print "Ok"
	else:
		print "Fail"
		print "Correct"
		print correct
		print "Result"
		print result

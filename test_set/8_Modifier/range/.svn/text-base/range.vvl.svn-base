def function(x, y):
	return x + y

def main():
	result = function(x, y).range(x=0:10,y=0:10)

	import numpy
	correct = numpy.empty((10,10))
	for i in range(10):
		for j in range(10):
			correct[i][j] = i + j

	if (correct == result).all():
		print "Ok"
	else:
		print "Fail"
		print "Correct"
		print correct
		print "Result"
		print result

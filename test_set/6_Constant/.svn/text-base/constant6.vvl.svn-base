def ct(c, d, a, b, x,y):
	return c + d + point_query_2d(a,x,y) + point_query_2d(b,x,y)

def main():
	import numpy
	n = 10
	a = numpy.ones((n,n),dtype=numpy.float32)
	b = numpy.ones((n,n),dtype=numpy.float32)*4
	
	result = ct(30, 40, a,b, x,y).range(x=0:n,y=0:n)
		
	correct = numpy.ones((n,n), dtype=numpy.float32)*75
	
	if (correct == result).all():
		print "Ok"
	else:
		print "Fail"
		print "Correct"
		print correct
		print "Result"
		print result

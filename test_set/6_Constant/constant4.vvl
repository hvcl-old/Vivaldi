def ct(c, d, x,y):
	return c + d

def main():
	import numpy
	n = 10
	
	result = ct(30, 40 ,x,y).range(x=0:n,y=0:n)
	
	correct = numpy.ones((n,n), dtype=numpy.float32)*70
	
	if (correct == result).all():
		print "Ok"
	else:
		print "Fail"
		print "Correct"
		print correct
		print "Result"
		print result
